{
  "cSpell.words": [
    "kysely",
    "preact",
    "twind",
    "deno",
    "favoriting",
    "upvoted"
  ],
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "eslint.enable": false,
  "prettier.trailingComma": "all",
  "prettier.useTabs": false,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 80,
  "prettier.singleQuote": false
}
